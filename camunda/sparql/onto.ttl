@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix tidit: <https://arge.innova.com.tr/Tidit.owl#> .
@prefix wot: <https://arge.innova.com.tr/wot.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Ontology
<https://arge.innova.com.tr/Tidit.owl>
    rdf:type owl:Ontology ;
    owl:imports <http://purl.obolibrary.org/obo/bfo/2014-05-03/bfo.owl> ;
    owl:imports <https://arge.innova.com.tr/wot.owl> ;
    owl:imports <https://arge.innova.com.tr/wot_mappings.owl#> .

# Object Properties
tidit:describe rdf:type owl:ObjectProperty ;
    rdfs:domain wot:Thing ;
    rdfs:range tidit:ThingDescription .

# Data properties
tidit:objectName rdf:type owl:DatatypeProperty ;
    rdfs:domain tidit:ThingDescription ;
    rdfs:range xsd:string .

tidit:detectionTime rdf:type owl:DatatypeProperty ;
    rdfs:domain tidit:ThingDescription ;
    rdfs:range xsd:dateTime .

tidit:confidence rdf:type owl:DatatypeProperty ;
    rdfs:domain tidit:ThingDescription ;
    rdfs:range xsd:float .

# Classes
tidit:ThingDescription rdf:type owl:Class .