{
  "@context": [
    "https://www.w3.org/2022/wot/td/v1.1",
    {
      "om2": "http://www.ontology-of-units-of-measure.org/resource/om-2/"
    }
  ],
  "@type": "tm:ThingModel",
  "title": "Power",
  "version": {
    "model": "1.0.0"
  },
  "tm:optional": [],
  "properties": {
    "Voltage": {
      "title": "Report Power",
      "description": "Configuration whether the voltage consumption should be published via event 'reportedPower'.",
      "type": "object",
      "properties": {
        "enabled": {
          "title": "Reporting enabled",
          "type": "boolean",
          "default": false
        }
      },
      "interval": {
          "@type": "time:Duration",
          "title": "Reporting interval",
          "type": "integer",
          "unit": "time:seconds",
          "default": 30
        }
    }
  },
  "events": {
    "reportedPower": {
      "title": "Power",
      "description": "Measurement of the power consumption.",
      "data": {
        "@type": "om2:Quantity",
        "title": "Consumption in power.",
        "type": "number",
        "om2:Unit": "om2:watt",
        "minimum": 0.0
      }
    }
  }
}